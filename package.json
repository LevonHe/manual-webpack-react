{
  "name": "manual-webapck-react",
  "version": "1.0.0",
  "description": "React web project with webpack4",
  "main": "index.js",
  "private": true,
  "repository": "https://github.com/LevonHe/manual-webpack-react.git",
  "scripts": {
    "test": "jest --colors --coverage",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.prod.js --progress --colors",
    "buildProd": "yarn run build --prod",
    "dev": "yarn run start"
  },
  "keywords": [
    "react",
    "webpack"
  ],
  "author": "heliuxi",
  "license": "MIT",
  "jest": {
    "testEnvironment": "jsdom",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(png|jpg|gif|jpeg|svg|woff|woff2|eot|ttf|otf)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.(css|less|scss)$": "<rootDir>/node_modules/jest-css-modules-transform"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "moduleFileExtensions": [
      "css",
      "less",
      "scss",
      "js",
      "json",
      "jsx"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.9.4",
    "autoprefixer": "^9.8.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cookie-parser": "^1.4.5",
    "copy-webpack-plugin": "^6.0.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.12.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-webpack-plugin": "^2.1.0",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-inline-css-webpack-plugin": "^1.8.1",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-css-modules-transform": "^3.1.0",
    "jest-enzyme": "^7.1.2",
    "jest-fetch-mock": "^2.1.2",
    "jsdom": "^15.2.1",
    "less": "^3.11.3",
    "less-loader": "^6.1.1",
    "merge": "^1.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "os": "^0.1.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "antd": "^3.26.16",
    "async-validator": "^3.3.0",
    "axios": "^0.19.2",
    "core-js": "^3.9.1",
    "crypto-js": "^4.0.0",
    "echarts": "^4.8.0",
    "echarts-for-react": "^3.0.1",
    "exif-rotate-js": "^1.3.0",
    "file-saver": "^2.0.2",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "mqtt": "^4.2.6",
    "net": "^1.0.2",
    "react": "^16.13.1",
    "react-ace-editor": "0.0.3",
    "react-copy-to-clipboard": "^5.0.2",
    "react-cropper": "^2.1.4",
    "react-dom": "^16.13.1",
    "react-intl-universal": "^2.2.5",
    "react-redux": "^5.0.6",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.7",
    "screenfull": "^5.1.0",
    "stompjs": "^2.3.3",
    "websocket": "^1.0.31"
  }
}
